{
  "welcomeFile": "/trialemmiui5-1.0.0/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "/srv_api/(.*)$",
      "target": "$1",
      "authenticationType": "none",
      "destination": "srv_api",
      "csrfProtection": false
    }, {
      "source": "^/app/(.*)$",
      "target": "/trialemmiui5-1.0.0/$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }, {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}